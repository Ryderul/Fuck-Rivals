                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })


    Tabs.Main:AddButton({
        Title = "Fps Booster",
        Description = "boost your fps NOTE: you cant turn this off",
        Callback = function()
            Window:Dialog({
                Title = "Fps Booster",
                Content = "Boosted Ts",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            
